{
    "name": "codebot/laravel-usub",
    "description": "Laravel package for authenticated user substitution to login as other users.",
    "authors": [
        {
            "name": "Sen",
            "email": "sen.hakobyan@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Usub\\": "src/"
        },
        "psr-0": {
            "Usub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra" : {
        "laravel": {
            "providers": [
                "Usub\\Core\\UsubServiceProvider"
            ]
        }
    }
}
